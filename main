#include "bmp8.h"

int main() {
    t_bmp8 *img = bmp8_loadImage("C:\\Users\\koumt\\Downloads\\barbara_gray.bmp"); // Remplace par un nom de fichier réel

    if (img) {
        bmp8_printInfo(img);
        bmp8_saveImage("copie.bmp", img);
        bmp8_free(img);
    } else {
        printf("Échec du chargement de l'image.\n");
    }

    return 0;
}